{
  "name": "redux-universal-example",
  "version": "0.0.0",
  "description": "An example of a universally-rendered Redux application",
  "scripts": {
    "test": "./node_modules/.bin/better-npm-run test",
    "start": "./node_modules/.bin/better-npm-run start",
    "start:dev": "./node_modules/.bin/better-npm-run start:dev",
    "build": "./node_modules/.bin/better-npm-run build",
    "build:dev": "./node_modules/.bin/better-npm-run build:dev",
    "build:prod": "./node_modules/.bin/better-npm-run build:prod",
    "prod": "./node_modules/.bin/better-npm-run prod",
    "dev": "./node_modules/.bin/better-npm-run dev"
  },
  "betterScripts": {
    "test": {
      "command": "mocha -R spec ./server/tests/test-*.js"
    },
    "start": {
      "command": "node server/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:dev": {
      "command": "node server/server-dev.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack"
    },
    "build:dev": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "prod": {
      "command": "npm run build:prod && npm run start",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "node server/server-dev.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactjs/redux.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactjs/redux/issues"
  },
  "homepage": "http://redux.js.org",
  "dependencies": {
    "babel-register": "^6.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-compress": "^2.0.0",
    "koa-connect": "^1.0.0",
    "koa-logger": "^2.0.0",
    "koa-mount": "^2.0.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "postcss-cssnext": "^2.6.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-lite": "^0.15.14",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "require-dir": "^0.3.0",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "^0.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "es-css-modules": "^1.2.3",
    "eslint": "^2.11.1",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "koa-static": "^3.0.0",
    "mocha": "^2.5.3",
    "postcss-loader": "^0.9.1",
    "postcss-modules": "^0.5.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "supertest-as-promised": "^3.1.0",
    "webpack": "^2.1.0-beta.13",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
